#include <iostream>
using namespace std;

int main(){
    char tab='\t';
    char sortores='\n';

    int tomb[20];

    for(int i=0; i<20; i++){
        cout<<tomb[i]<<sortores;
    }

   for(int i=0; i<20; i++){
        tomb[i]=(i+1)*3;
    }

    for(int i=0; i<20; i++){
        cout<<tomb[i]<<sortores;
    }

    for(int i=0; i<20; i++){
        tomb[i]=(i+1)*4;
    }

   for(int i=0; i<20; i++){
        cout<<tomb[i]<<sortores;
    }




    for(int i=0; i<20; i++){
        tomb[i]=(i%6)+1;
    }

     for(int i=0; i<20; i++){
        cout<<tomb[i]<<sortores;
    }

    cout<<"Tab elott"<<tab<<"Tab utan"<<sortores;

    cout<<"Sortores elott"<<sortores<<"Sortores utan"<<sortores;




    for(int i=1; i<5+1; i++){
        for(int j=i; j<5+1; j++){
            cout<<1;
        }
        cout<< endl;
    }

    for(int i=1; i<5+1; i++){
        for(int j=1; j<i+1; j++){
            cout<<1;
        }
        cout<< endl;
    }



cout<<"Lefutottam"<<sortores;
    return 0;
}
